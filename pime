#include<iostream>
using namespace std;

class p
{
	int i,num,count=0;
	public:
		prime()
		   {
		   	 cout<<"Enter number ";
		   	 cin>>num;
		   	 for(i=2;i<=num;i++)
		   	    {
		   	    	if(num%i==0)
		   	    	{
		   	    		count ++;
		   	    	}
		   	    }
			  if(count == 1)
			  {
		   	    cout<<"prime";	
		   	  }
			  else 
				cout<<"not prime";
				
		
	}
};

int main()
{
	p obj;
	obj.prime();
	return 0;
}
